{
  "name": "studymate-platform",
  "version": "1.0.0",
  "description": "A comprehensive educational platform with user authentication, collaborative learning features, file sharing, event management, and real-time messaging",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "react-start": "cd studymate-react && npm start",
    "react-build": "cd studymate-react && npm run build",
    "install-all": "npm install && cd studymate-react && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "education",
    "platform",
    "learning",
    "collaboration",
    "file-sharing",
    "events",
    "messaging",
    "express",
    "nodejs"
  ],
  "author": "Studymate Team",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  }
}
